---
locale: es_ES.UTF-8
timezone: Europe/Madrid
swap:
  size: 1024M
  settings:
    - { key: vm.swappiness, value: 10 }
    - { key: vm.vfs_cache_pressure, value: 50 }
ssh:
  enable_forward_agent: true
packages: []
mysql:
  packages: [mariadb-client, mariadb-server, python-mysqldb]
  databases: [test]
  users:
    - { name: test, password: test, database: test}
